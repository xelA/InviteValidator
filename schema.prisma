datasource db {
  provider = "sqlite"
  url      = "file:./storage.db"
}

model whitelist {
  guild_id      BigInt  @id @unique
  whitelist     Boolean @default(true)
  granted_by    BigInt
  revoked_by    BigInt?
  banned        Boolean @default(false)
  banned_by     BigInt?
  banned_reason String?
  invited       Boolean @default(false)
}
